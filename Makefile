CC = g++
LIBS = -lm
SRC = src
OBJ = obj
INC = include
BIN = bin
OBJS = $(OBJ)/main.o $(OBJ)/lista.o $(OBJ)/satisfabilidade.o $(OBJ)/avaliacao.o $(OBJ)/tipo_item.o $(OBJ)/item_arvore.o
HDRS = $(INC)/lista.h $(INC)/satisfabilidade.h $(INC)/avaliacao.h $(INC)/tipo_item.h $(INC)/item_arvore.h
CFLAGS = -Wall -c -I$(INC)

EXE = $(BIN)/tp1.out

